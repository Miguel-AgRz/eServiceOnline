@using eServiceOnline.Models.CrewBoard
@model eServiceOnline.Models.ThirdPartyCrewBoard.ThirdPartyCrewModel

@using (Html.BeginForm("RemoveThirdPartyCrew", "ThirdPartyCrewBoard", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.HiddenFor(m => m.Id)
    <div>
        <div style="margin-top: 20px; text-align: center;" id="div_display_notes">
            <lable>Are you sure you want to remove the ThirdPartyCrew?</lable>
        </div>
        <div style="text-align: center; margin: auto; line-height: 100px;">
            <div class="sj_dialog_foot">
                <input type="submit" id="submitButton" class="yes" value="Yes"/> <input type="button" value="No" class="no" onclick="siteInstance.CloseConfirmDialog()" />
            </div>
        </div>
    </div>
}
